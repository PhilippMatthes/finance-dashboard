            },
            "editorMode": "code",
            "format": "table",
            "rawQuery": true,
            "rawSql": "WITH total_income AS (\n    SELECT \n        SUM(amount) AS total_income\n    FROM \n        transactions\n    WHERE \n        amount > 0\n        AND date BETWEEN $__timeFrom() AND $__timeTo()\n),\ntotal_expenses AS (\n    SELECT \n        SUM(amount) AS total_expenses\n    FROM \n        transactions\n    WHERE \n        amount < 0\n        AND date BETWEEN $__timeFrom() AND $__timeTo()\n),\nexpenses_by_class AS (\n    SELECT \n        primary_class,\n        ABS(SUM(amount)) AS total_amount\n    FROM \n        transactions\n    WHERE \n        amount < 0\n        AND date BETWEEN $__timeFrom() AND $__timeTo()\n    GROUP BY \n        primary_class\n    HAVING \n        SUM(amount) <> 0\n)\nSELECT \n    primary_class,\n    total_amount\nFROM \n    expenses_by_class\nUNION ALL\nSELECT \n    'Free Income' AS primary_class,\n    (SELECT total_income FROM total_income) + (SELECT total_expenses FROM total_expenses) AS total_amount\nORDER BY \n    total_amount DESC;",
            "refId": "A",
