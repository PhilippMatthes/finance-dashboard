            "editorMode": "code",
            "format": "table",
            "rawQuery": true,
            "rawSql": "SELECT \n    DATE_TRUNC('month', date) AS month,\n    SUM(CASE WHEN amount > 0 THEN amount ELSE 0 END) AS Income,\n    -SUM(CASE WHEN amount < 0 THEN amount ELSE 0 END) AS Expenses\nFROM \n    transactions\nWHERE \n    date BETWEEN $__timeFrom() AND $__timeTo()\nGROUP BY \n    DATE_TRUNC('month', date)\nORDER BY \n    month;",
